    <script>
        <scripts>
            <name>SOAP Support.js</name>
            <description>Script for representing each SOAP operation uniquely in the Sites Tree.</description>
            <engine>Oracle Nashorn</engine>
            <type>variant</type>
            <enabled>true</enabled>
            <file>/root/.ZAP/scripts/scripts/variant/SOAP Support.js</file>
        </scripts>
        <scripts>
            <name>GraphQL Support.js</name>
            <description>Input Vectors Script for injecting inline arguments in GraphQL queries proxied through ZAP.</description>
            <engine>Oracle Nashorn</engine>
            <type>variant</type>
            <enabled>true</enabled>
            <file>/root/.ZAP/scripts/scripts/variant/GraphQL Support.js</file>
        </scripts>
    </script>
